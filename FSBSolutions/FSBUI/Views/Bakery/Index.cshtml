                <div class="clearfix"></div>
                <input type="hidden" value="@ViewBag.LineId" id="lineid" />
                <input type="hidden" value="@ViewBag.UserTypeId" id="usertypeid" />
                <input type="hidden" value="@ViewBag.UserId" id="userid" />
                <input type="hidden" value="" id="prodstarttime" />
                <input type="hidden" value="0" id="plantid" />
                <div class="bakeryAccordionData mobileXscroll pl-4 pr-4 pb-4">
                    <div class="row">
                        <div class="col-lg-1 col-md-1">
                            <span class="lh-40">@DateTime.Now.AddDays(-3).ToString("dd/MM/yyyy")</span>
                        </div>
                        <div class="col-lg-11 col-md-11"><div class="w-25 productionRow1 float-left fs-regular-14 fs-regular-sm-12 fc-grey pl-1 text-wrap d-flex align-center">Hamburger</div><div class="w-50 productionRow2 float-left fs-regular-14 fs-regular-sm-12 fc-grey pl-1 text-wrap d-flex align-center">McRib</div><div class="w-25 productionRow3 float-left fs-regular-14 fs-regular-sm-12 fc-grey pl-1 text-wrap d-flex align-center">Jerra Brot</div></div>
                    </div>
                    <div class="row mt-2">
                        <div class="col-lg-1 col-md-1">
                            <span class="lh-40">@DateTime.Now.AddDays(-2).ToString("dd/MM/yyyy")</span>
                        </div>
                        <div class="col-lg-11 col-md-11"><div class="w-50 productionRow2 float-left fs-regular-14 fs-regular-sm-12 fc-grey pl-1 text-wrap d-flex align-center">McRib</div><div class="w-50 productionRow1 float-left fs-regular-14 fs-regular-sm-12 fc-grey pl-1 text-wrap d-flex align-center">Hamburger</div></div>
                    </div>
                    <div class="row mt-2">
                        <div class="col-lg-1 col-md-1">
                            <span class="lh-40">@DateTime.Now.AddDays(-1).ToString("dd/MM/yyyy")</span>
                        </div>
                        <div class="col-lg-11 col-md-11"><div class="w-100 productionRow3 float-left fs-regular-14 fs-regular-sm-12 fc-grey pl-1 text-wrap d-flex align-center">Jeera Brot</div></div>
                    </div>
                </div>

            </div>
        </div>
    </div>


    <div class="row mt-4">

